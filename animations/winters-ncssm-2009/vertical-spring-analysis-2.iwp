				</graphOptions>
			</shape>
			<color>
				<red>255</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="euler">
					<displacement>0</displacement>
					<velocity>0</velocity>
					<acceleration>0</acceleration>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="euler">
					<displacement>a</displacement>
					<velocity>0</velocity>
					<acceleration>-ball.ypos*k/m</acceleration>
				</calculator>
			</ypath>
		</solid>
		<output>
			<name>p</name>
			<text>Spring pitch</text>
			<units>m</units>
			<calculator type="parametric">
				<value>(-ball.ypos+anchor)/6</value>
			</calculator>
			<hidden>1</hidden>
		</output>
		<output>
			<name>yeq</name>
			<text>Relaxed position</text>
			<units>m</units>
			<calculator type="parametric">
				<value>m*g/k</value>
