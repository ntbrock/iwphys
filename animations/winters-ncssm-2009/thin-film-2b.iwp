			<units></units>
			<hidden>1</hidden>
		</input>
		<output>
			<name>d</name>
			<text>Thickness of film</text>
			<units>nm</units>
			<calculator type="parametric">
				<value>do+di*t</value>
			</calculator>
		</output>
		<output>
			<name>w</name>
			<text>Wavelength</text>
			<units>nm</units>
			<calculator type="parametric">
				<value>wo+wi*t</value>
			</calculator>
			<hidden>1</hidden>
		</output>
		<output>
			<name>ar</name>
			<text>Angle of refraction 1</text>
			<units>rad</units>
			<calculator type="parametric">
				<value>asin(n1*sin(toRadians(a))/n2)</value>
			</calculator>
			<hidden>1</hidden>
		</output>
		<output>
			<name>pd</name>
			<text>Phase difference</text>
			<units>w</units>
			<calculator type="parametric">
				<value>2*d*n2/(w*cos(ar))+step(n3/n2-1)/2-step(n2/n1-1)/2</value>
			</calculator>
			<hidden>1</hidden>
		</output>
