<?xml version="1.0"?>

<problem>
	<author>
		<username></username>
		<name></name>
		<organization></organization>
		<email></email>
	</author>
	<objects>
		<time>
			<start>0.0</start>
			<stop>10.0</stop>
			<change>0.1</change>
			<fps>10.0</fps>
		</time>
		<GraphWindow>
			<xmin>0.0</xmin>
			<xmax>100.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>2.0</xgrid>
			<ygrid>2.0</ygrid>
		</GraphWindow>
		<window>
			<xmin>-10.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>2.0</xgrid>
			<ygrid>2.0</ygrid>
			<xunit>meters</xunit>
			<yunit>meters</yunit>
		</window>
		<description>
			<text>In order to make this work, these variables must be entered in the wavebox properties. Note that this can not be done in the client application.

For Vx, enter vs.
For Frequency, enter f.
For Wavelength, enter vw/f.
</text>
		</description>
		<object class="edu.ncssm.iwp.objects.wavebox.DObject_WaveBox">
			<name>New_WaveBox</name>
			<color>
				<red>0</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<minX>
				<calculator type="parametric">
					<value>-10</value>
				</calculator>
			</minX>
			<maxX>
				<calculator type="parametric">
					<value>10</value>
				</calculator>
			</maxX>
			<minY>
				<calculator type="parametric">
					<value>-10</value>
				</calculator>
			</minY>
			<maxY>
				<calculator type="parametric">
					<value>10</value>
				</calculator>
			</maxY>
			<waveGenerator>
				<X>
					<calculator type="parametric">
						<value>0.0</value>
					</calculator>
				</X>
				<Y>
					<calculator type="parametric">
						<value>0.0</value>
					</calculator>
				</Y>
				<Width>
					<calculator type="parametric">
						<value>10.0</value>
					</calculator>
				</Width>
				<Height>
					<calculator type="parametric">
						<value>10.0</value>
					</calculator>
				</Height>
				<XVelocity>
					<calculator type="parametric">
						<value>vs</value>
					</calculator>
				</XVelocity>
				<YVelocity>
					<calculator type="parametric">
						<value>0.0</value>
					</calculator>
				</YVelocity>
				<Doppler>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</Doppler>
				<waveIterations>
					<calculator type="parametric">
						<value>10</value>
					</calculator>
				</waveIterations>
				<numWaves>
					<calculator type="parametric">
						<value>10</value>
					</calculator>
				</numWaves>
				<Frequency>
					<calculator type="parametric">
						<value>f</value>
					</calculator>
				</Frequency>
				<Wavelength>
					<calculator type="parametric">
						<value>vw/f</value>
					</calculator>
				</Wavelength>
				<Period>
					<calculator type="parametric">
						<value>5</value>
					</calculator>
				</Period>
				<maxAmplitude>
					<calculator type="parametric">
						<value>5.0</value>
					</calculator>
				</maxAmplitude>
				<phaseShift>
					<calculator type="parametric">
						<value>0.0</value>
					</calculator>
				</phaseShift>
				<leftToRight>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</leftToRight>
			</waveGenerator>
		</object>
		<input>
			<name>vs</name>
			<text>Velocity of Source</text>
			<initialValue>1.0</initialValue>
			<units>m/s</units>
		</input>
		<input>
			<name>vw</name>
			<text>Velocity of waves</text>
			<initialValue>2.0</initialValue>
			<units>m/s</units>
		</input>
		<input>
			<name>f</name>
			<text>Unshifted frequency</text>
			<initialValue>1.0</initialValue>
			<units>1/s</units>
		</input>
	</objects>
</problem>
