<?xml version="1.0"?>

<problem>
	<author>
		<username></username>
		<name>Taylor Brockman</name>
		<organization>IWP</organization>
		<email>taylor.brockman@gmail.com</email>
	</author>
	<objects>
		<time>
			<start>0.0</start>
			<stop>10.0</stop>
			<change>0.02</change>
			<fps>15.0</fps>
		</time>
		<GraphWindow>
			<xmin>0.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-15.0</ymin>
			<ymax>15.0</ymax>
			<xgrid>1.5</xgrid>
			<ygrid>1.5</ygrid>
		</GraphWindow>
		<window>
			<xmin>-10.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>1.0</xgrid>
			<ygrid>1.0</ygrid>
			<xunit>meters</xunit>
			<yunit>meters</yunit>
		</window>
		<description>
			<text>This is a problem that tests graphing of shapes.</text>
		</description>
		<solid>
			<name>DispMover</name>
			<shape type="circle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</height>
				<graphOptions graphVisible="true">
					<initiallyOn yAccel="false" yVel="false" xAccel="false" xVel="false" yPos="true" xPos="true"/>
				</graphOptions>
			</shape>
			<color>
				<red>255</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>10*sin(t)</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>10*cos(t)</value>
				</calculator>
			</ypath>
		</solid>


		<solid>
			<name>VelocityMover</name>
			<shape type="circle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</height>
				<graphOptions graphVisible="true">
					<initiallyOn yAccel="false" yVel="true" xAccel="false" xVel="true" yPos="false" xPos="false"/>
				</graphOptions>
			</shape>
			<color>
				<red>0</red>
				<green>255</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>9*sin(t*2)</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>9*cos(t*2)</value>
				</calculator>
			</ypath>
		</solid>


		<solid>
			<name>AccelMover</name>
			<shape type="circle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>1</value>
					</calculator>
				</height>
				<graphOptions graphVisible="true">
					<initiallyOn yAccel="true" yVel="true" xAccel="false" xVel="false" yPos="false" xPos="false"/>
				</graphOptions>
			</shape>
			<color>
				<red>0</red>
				<green>0</green>
				<blue>255</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>8*sin(t*4)</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>8*cos(t*4)</value>
				</calculator>
			</ypath>
		</solid>

	</objects>
</problem>
