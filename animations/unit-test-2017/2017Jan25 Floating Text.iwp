<?xml version="1.0"?>

<problem>
	<author>
		<username></username>
		<name></name>
		<organization></organization>
		<email></email>
	</author>
	<objects>
		<time>
			<start>0.0</start>
			<stop>10.0</stop>
			<change>0.1</change>
			<fps>10.0</fps>
		</time>
		<GraphWindow>
			<xmin>0.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>2.0</xgrid>
			<ygrid>2.0</ygrid>
		</GraphWindow>
		<window>
			<xmin>-10.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>2.0</xgrid>
			<ygrid>2.0</ygrid>
			<xunit>m</xunit>
			<yunit>m</yunit>
			<showAllDataAvailable>false</showAllDataAvailable>
			<drawGridNumbers>true</drawGridNumbers>
		</window>
		<description>
			<text>Created as a developer unit test to flex floating text rendering. Taylor + Ryan</text>
		</description>
		<object class="edu.ncssm.iwp.objects.floatingtext.DObject_FloatingText">
			<name>floater</name>
			<text>Floating</text>
			<units>time</units>
			<value>
				<calculator type="parametric">
					<value>t*2*t*4</value>
				</calculator>
			</value>
			<fontSize>14</fontSize>
			<showValue>true</showValue>
			<color>
				<red>50</red>
				<green>100</green>
				<blue>150</blue>
			</color>
			<ypath>
				<calculator type="parametric">
					<value>t*4</value>
				</calculator>
			</ypath>
			<xpath>
				<calculator type="parametric">
					<value>t*2</value>
				</calculator>
			</xpath>
		</object>
	</objects>
</problem>
