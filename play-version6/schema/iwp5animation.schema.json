{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Interactive Web Physics Version 5",
  "required": [
    "author",
    "objects"
  ],
  "properties": {
    "author": {
      "$id": "#/properties/author",
      "type": "object",
      "title": "The Author Schema",
      "required": [
        "username",
        "name",
        "organization",
        "email"
      ],
      "properties": {
        "username": {
          "$id": "#/properties/author/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "default": "",
          "examples": [
            "winters@ncssm.edu"
          ],
          "pattern": "^(.*)$"
        },
        "name": {
          "$id": "#/properties/author/properties/name",
          "type": "object",
          "title": "The Name Schema"
        },
        "organization": {
          "$id": "#/properties/author/properties/organization",
          "type": "object",
          "title": "The Organization Schema"
        },
        "email": {
          "$id": "#/properties/author/properties/email",
          "type": "object",
          "title": "The Email Schema"
        }
      }
    },
    "objects": {
      "$id": "#/properties/objects",
      "type": "object",
      "title": "The Objects Schema",
      "required": [
        "time",
        "GraphWindow",
        "window",
        "description",
        "input",
        "solid",
        "output"
      ],
      "properties": {
        "time": {
          "$id": "#/properties/objects/properties/time",
          "type": "object",
          "title": "The Time Schema",
          "required": [
            "start",
            "stop",
            "change",
            "fps"
          ],
          "properties": {
            "start": {
              "$id": "#/properties/objects/properties/time/properties/start",
              "type": "string",
              "title": "The Start Schema",
              "default": "",
              "examples": [
                "0.0"
              ],
              "pattern": "^(.*)$"
            },
            "stop": {
              "$id": "#/properties/objects/properties/time/properties/stop",
              "type": "string",
              "title": "The Stop Schema",
              "default": "",
              "examples": [
                "0.14"
              ],
              "pattern": "^(.*)$"
            },
            "change": {
              "$id": "#/properties/objects/properties/time/properties/change",
              "type": "string",
              "title": "The Change Schema",
              "default": "",
              "examples": [
                "0.01"
              ],
              "pattern": "^(.*)$"
            },
            "fps": {
              "$id": "#/properties/objects/properties/time/properties/fps",
              "type": "string",
              "title": "The Fps Schema",
              "default": "",
              "examples": [
                "20.0"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "GraphWindow": {
          "$id": "#/properties/objects/properties/GraphWindow",
          "type": "object",
          "title": "The Graphwindow Schema",
          "required": [
            "xmin",
            "xmax",
            "ymin",
            "ymax",
            "xgrid",
            "ygrid"
          ],
          "properties": {
            "xmin": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/xmin",
              "type": "string",
              "title": "The Xmin Schema",
              "default": "",
              "examples": [
                "0.0"
              ],
              "pattern": "^(.*)$"
            },
            "xmax": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/xmax",
              "type": "string",
              "title": "The Xmax Schema",
              "default": "",
              "examples": [
                "0.1"
              ],
              "pattern": "^(.*)$"
            },
            "ymin": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/ymin",
              "type": "string",
              "title": "The Ymin Schema",
              "default": "",
              "examples": [
                "-0.1"
              ],
              "pattern": "^(.*)$"
            },
            "ymax": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/ymax",
              "type": "string",
              "title": "The Ymax Schema",
              "default": "",
              "examples": [
                "0.1"
              ],
              "pattern": "^(.*)$"
            },
            "xgrid": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/xgrid",
              "type": "string",
              "title": "The Xgrid Schema",
              "default": "",
              "examples": [
                "0.01"
              ],
              "pattern": "^(.*)$"
            },
            "ygrid": {
              "$id": "#/properties/objects/properties/GraphWindow/properties/ygrid",
              "type": "string",
              "title": "The Ygrid Schema",
              "default": "",
              "examples": [
                "0.01"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "window": {
          "$id": "#/properties/objects/properties/window",
          "type": "object",
          "title": "The Window Schema",
          "required": [
