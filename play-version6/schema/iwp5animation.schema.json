                      },
                      "drawVectors": {
                        "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/@attributes/properties/drawVectors",
                        "type": "string",
                        "title": "The Drawvectors Schema",
                        "default": "",
                        "examples": [
                          "false"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  },
                  "vectors": {
                    "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors",
                    "type": "object",
                    "title": "The Vectors Schema",
                    "required": [
                      "@attributes"
                    ],
                    "properties": {
                      "@attributes": {
                        "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes",
                        "type": "object",
                        "title": "The @attributes Schema",
                        "required": [
                          "yAccel",
                          "Vel",
                          "xAccel",
                          "yVel",
                          "xVel",
                          "Accel"
                        ],
                        "properties": {
                          "yAccel": {
                            "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes/properties/yAccel",
                            "type": "string",
                            "title": "The Yaccel Schema",
                            "default": "",
                            "examples": [
                              "false"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "Vel": {
                            "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes/properties/Vel",
                            "type": "string",
                            "title": "The Vel Schema",
                            "default": "",
                            "examples": [
                              "false"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "xAccel": {
                            "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes/properties/xAccel",
                            "type": "string",
                            "title": "The Xaccel Schema",
                            "default": "",
                            "examples": [
                              "false"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "yVel": {
                            "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes/properties/yVel",
                            "type": "string",
                            "title": "The Yvel Schema",
                            "default": "",
                            "examples": [
                              "false"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "xVel": {
                            "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/vectors/properties/@attributes/properties/xVel",
                            "type": "string",
                            "title": "The Xvel Schema",
                            "default": "",
                            "examples": [
                              "false"
                            ],
                            "pattern": "^(.*)$"
                          },
