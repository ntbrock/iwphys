                                "type": "string",
                                "title": "The Yaccel Schema",
                                "default": "",
                                "examples": [
                                  "false"
                                ],
                                "pattern": "^(.*)$"
                              },
                              "yVel": {
                                "$id": "#/properties/objects/properties/solid/items/properties/shape/properties/graphOptions/properties/initiallyOn/properties/@attributes/properties/yVel",
                                "type": "string",
                                "title": "The Yvel Schema",
                                "default": "",
