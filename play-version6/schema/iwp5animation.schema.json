          "type": "array",
          "title": "The Solid Schema",
          "items": {
            "$id": "#/properties/objects/properties/solid/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "name",
              "shape",
              "color",
              "xpath",
              "ypath"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/objects/properties/solid/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
