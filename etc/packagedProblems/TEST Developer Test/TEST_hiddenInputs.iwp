<?xml version="1.0"?>

<problem>
	<author>
		<username></username>
		<name>Taylor Brockman</name>
		<organization>IWP</organization>
		<email>taylor.brockman@gmail.com</email>
	</author>
	<objects>
		<time>
			<start>0.0</start>
			<stop>100.0</stop>
			<change>0.02</change>
			<fps>15.0</fps>
		</time>
		<GraphWindow>
			<xmin>-10.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>1.0</xgrid>
			<ygrid>1.0</ygrid>
		</GraphWindow>
		<window>
			<xmin>-10</xmin>
			<xmax>10</xmax>
			<ymin>-10</ymin>
			<ymax>10</ymax>
			<xgrid>.5</xgrid>
			<ygrid>.5</ygrid>
			<xunit>meters</xunit>
			<yunit>meters</yunit>
		</window>
		<description>
			<text>TEST for hidden inputs. Make sure that the value of the hidden input
			is set to the initial value on initial problem load, and not just reset.</text>
		</description>

		<input>
			<name>inputone</name>
			<text>Input One</text>
			<initialValue>1</initialValue>
			<units>u</units>
			<hidden>1</hidden>
		</input>

		<output>
			<name>outputone</name>
			<text>Output One</text>
			<units>u</units>
			<calculator type="parametric">
				<value>inputone</value>
			</calculator>
		</output>

		<solid>
			<name>solidone</name>
			<shape type="rectangle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>5</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>5</value>
					</calculator>
				</height>
				
				<graphOptions graphVisible="false">
					<initiallyOn yAccel="false" yVel="false" xAccel="false" xVel="false" yPos="false" xPos="false"/>
				</graphOptions>
			</shape>
			<color>
				<red>255</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>inputone</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>outputone</value>
				</calculator>
			</ypath>
		</solid>




		<input>
			<name>inputtwo</name>
			<text>Input Two</text>
			<initialValue>1</initialValue>
			<units>u</units>
			<hidden>0</hidden>
		</input>

		<output>
			<name>outputtwo</name>
			<text>Output Two</text>
			<units>u</units>
			<calculator type="parametric">
				<value>inputtwo</value>
			</calculator>
		</output>
		
		
		<solid>
			<name>solidtwo</name>
			<shape type="rectangle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>5</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>5</value>
					</calculator>
				</height>
				
				<graphOptions graphVisible="false">
					<initiallyOn yAccel="false" yVel="false" xAccel="false" xVel="false" yPos="false" xPos="false"/>
				</graphOptions>
			</shape>
			<color>
				<red>0</red>
				<green>255</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>inputtwo</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>outputtwo</value>
				</calculator>
			</ypath>
		</solid>
		
		

	</objects>
</problem>
