<?xml version="1.0"?>

<problem>
	<author>
		<username></username>
		<name>Taylor Brockman</name>
		<organization>IWP</organization>
		<email>taylor.brockman@gmail.com</email>
	</author>
	<objects>
		<time>
			<start>0.0</start>
			<stop>10.0</stop>
			<change>0.5</change>
			<fps>15.0</fps>
		</time>
		<GraphWindow>
			<xmin>0.0</xmin>
			<xmax>10.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>1.5</xgrid>
			<ygrid>1.5</ygrid>
		</GraphWindow>
		<window>
			<xmin>0.0</xmin>
			<xmax>100.0</xmax>
			<ymin>-10.0</ymin>
			<ymax>10.0</ymax>
			<xgrid>10.0</xgrid>
			<ygrid>10.0</ygrid>
			<xunit>meters</xunit>
			<yunit>meters</yunit>
		</window>
		<description>
			<text>This is a problem that tests the various kinds of shapes.</text>
		</description>
		<solid>
			<name>Circle</name>
			<shape type="circle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>15</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>1.5*t</value>
					</calculator>
				</height>
			</shape>
			<color>
				<red>50</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>10</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>0</value>
				</calculator>
			</ypath>
		</solid>


		<solid>
			<name>Rectangle</name>
			<shape type="rectangle" drawTrails="false">
				<width>
					<calculator type="parametric">
						<value>15</value>
					</calculator>
				</width>
				<height>
					<calculator type="parametric">
						<value>1.5t</value>
					</calculator>
				</height>
			</shape>
			<color>
				<red>100</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>30</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>0</value>
				</calculator>
			</ypath>
		</solid>



		<solid>
			<name>Polygon</name>
			<shape type="polygon" drawTrails="false">

				<points>
					<point index="0">
						<xpath>
							<calculator type="parametric">
								<value>-5</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>t</value>
							</calculator>
						</ypath>
					</point>
					<point index="1">
						<xpath>
							<calculator type="parametric">
								<value>-5</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>-t</value>
							</calculator>
						</ypath>
					</point>
					<point index="2">
						<xpath>
							<calculator type="parametric">
								<value>0</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>-5</value>
							</calculator>
						</ypath>
					</point>
					<point index="3">
						<xpath>
							<calculator type="parametric">
								<value>5</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>-t</value>
							</calculator>
						</ypath>
					</point>
					<point index="4">
						<xpath>
							<calculator type="parametric">
								<value>5</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>t</value>
							</calculator>
						</ypath>
					</point>
					<point index="5">
						<xpath>
							<calculator type="parametric">
								<value>0</value>
							</calculator>
						</xpath>
						<ypath>
							<calculator type="parametric">
								<value>5</value>
							</calculator>
						</ypath>
					</point>
				</points>
			</shape>
			<color>
				<red>200</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>50</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>0</value>
				</calculator>
			</ypath>
		</solid>


		<solid>
			<name>Line</name>
			<shape type="line" drawTrails="false">
				<height>
					<calculator type="parametric">
						<value>8+8*sin(t/4)</value>
					</calculator>
				</height>
				<width>
					<calculator type="parametric">
						<value>10</value>
					</calculator>
				</width>
			</shape>
			<color>
				<red>250</red>
				<green>0</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>65</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>-8</value>
				</calculator>
			</ypath>
		</solid>


		<solid>
			<name>Vector</name>
			<shape type="vector" drawTrails="false">
				<height>
					<calculator type="parametric">
						<value>10*sin(t/4)</value>
					</calculator>
				</height>
				<width>
					<calculator type="parametric">
						<value>10*cos(t/4)</value>
					</calculator>
				</width>
			</shape>
			<color>
				<red>255</red>
				<green>100</green>
				<blue>0</blue>
			</color>
			<xpath>
				<calculator type="parametric">
					<value>90</value>
				</calculator>
			</xpath>
			<ypath>
				<calculator type="parametric">
					<value>0</value>
				</calculator>
			</ypath>
		</solid>




	</objects>
</problem>
